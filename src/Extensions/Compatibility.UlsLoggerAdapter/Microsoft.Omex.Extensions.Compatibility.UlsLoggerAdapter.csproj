<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>$(LibraryTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup Label="NuGet Properties">
    <Title>Microsoft.Omex.Extensions.Compatibility.UlsLoggerAdapter</Title>
    <Summary>Microsoft Omex Extensions Compatibility UlsLoggerAdapter</Summary>
    <Description>This library contains compatibility classes that enable logging using the legacy Omex extensions.</Description>
    <ReleaseNotes>Initial release.</ReleaseNotes>
    <PackageProjectUrl>https://github.com/microsoft/Omex/tree/main/src/Extensions/Compatibility.UlsLoggerAdapter</PackageProjectUrl>
    <PackageTags>Microsoft;Omex;Extensions;UlsLoggerAdapter</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\System\Microsoft.Omex.System.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="$(Microsoft_Extensions_Hosting_Abstractions_Version)" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="$(Microsoft_Extensions_Logging_Abstractions_Version)" />
  </ItemGroup>
</Project>
