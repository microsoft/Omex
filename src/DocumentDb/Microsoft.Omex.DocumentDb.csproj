<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>$(NetStandardVersion)</TargetFrameworks>
    <Description>Contains Omex Document Db code</Description>
    <RootNamespace>Microsoft.Omex.DocumentDb</RootNamespace>
    <AssemblyName>Microsoft.Omex.DocumentDb</AssemblyName>
  </PropertyGroup>
  <PropertyGroup Label="NuGet Properties">
    <Title>Microsoft.Omex.DocumentDb</Title>
    <DevelopmentDependency>True</DevelopmentDependency>
    <Authors>Microsoft</Authors>
    <Summary>Microsoft Omex DocumentDb Wrappers</Summary>
    <Description>This library contains wrapper APIs over Microsoft Azure Document Db .Net Client SDK</Description>
    <ReleaseNotes>Initial release.</ReleaseNotes>
    <PackageProjectUrl>https://github.com/microsoft/Omex/tree/master/src/DocumentDb</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/microsoft/Omex</RepositoryUrl>
    <Copyright>© Microsoft Corporation. All rights reserved.</Copyright>
    <PackageTags>Microsoft;Omex;Gating</PackageTags>
  </PropertyGroup>
  <ItemGroup Condition="'$(TargetFramework)' == '$(NetStandardVersion)'">
    <PackageReference Include="Microsoft.Azure.DocumentDB.Core" Version="$(MicrosoftAzureDocumentDbVersionCore)" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="$(SystemConfigurationConfigurationManager)" />
  </ItemGroup>
    <ItemGroup>
    <ProjectReference Include="..\..\src\System\Microsoft.Omex.System.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Scripts\" />
  </ItemGroup>
</Project>